<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Authors>mkenfenheuer</Authors>
    <Copyright>Maximilian Kenfenheuer</Copyright>
    <PackageProjectUrl>https://github.com/mKenfenheuer/KSol.MySQLBackupLib.Pbs</PackageProjectUrl>
    <RepositoryUrl>https://github.com/mKenfenheuer/KSol.MySQLBackupLib.Pbs</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>mysql,backup,proxmox,pbs</PackageTags>
    <PackageLicenseExpression>GPL-3.0-only</PackageLicenseExpression>
    <Description>.Net Extension to enable Mysql Backups using KSol.MySQLBackupLib to Proxmox Backup Server </Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="KSol.MySQLBackupLib" Version="1.0.0" />
    <PackageReference Include="PbsClientDotNet" Version="1.0.1" />
  </ItemGroup>

</Project>
